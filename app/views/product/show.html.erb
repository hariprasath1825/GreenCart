<div style="margin-left: 400px ; margin-top: 50px">

  <div class="ui horizontal card" style="height: 400px; width: 1000px">
    <div class="image style_image" style="height: 400px ; width: 400px; background-color: white ; object-fit: contain">

      <%if @product.image.attached? %>
        <div id='banner'><%= image_tag @product.image,  :alt=> "banner", :size => '400X400'%></div>
      <%else %>
        <img src = "data:image/png;base64,">
      <%end %>
    </div>

    <div class="content" style="width: 500px; font-size: 1.8em ; background-color: whitesmoke">
      <div class="header" style="font-size: 2.5em"><%=@product.name.humanize %>
        <%unless @avg_rating==0 %>
          <p style="font-size: 0.4em; font-weight: normal ; color: green;  margin-left: 300px; margin-top: -50px">Avg Rating : <%=@avg_rating %></p>
        <%end %>
      </div>
      <div class="description" style="margin-top: 10px">
        <p class="category">Category : <%=@product.category %></p>
        <p>Description : <%=@product.description %></p>
        <p>Available Quantity : <%=@product.available_quantity %> kg</p>
        <%if current_user.customer? %>
          <p>Seller name : <%=@product.seller.name.humanize %></p><br>
        <%else %>
          <br><br>
        <%end %>
        <div>
          <%=link_to 'Back',product_index_path  , class:"huge ui grey button" , style:"margin-left:30px  ;margin-right: 80px"%>
          <%if current_user.customer? and @product.available_quantity.to_i != 0%>
            <%=link_to 'Add to Cart' , new_product_cart_path(product_id:  @product.id), class:"huge ui green button" %>
          <%end %>

          <%if current_user.seller? %>
            <%=link_to 'Edit' , edit_product_path(product_id:  @product.id), class:"huge ui teal button" %>
          <%end %>
        </div>

      </div>
    </div>
  </div>

</div>
<br>
<br>


<%if @reviews.nil? %>


<%else %>
  <div class="ui comments">
    <%@reviews.each do |review|  %>
      <div class="ui horizontal card" style="margin-left: 700px ;height: 150px; width: 500px ; background-color: rgba(170, 202, 198, 0.2)">

        <a class="avatar">
          <!--          <img src=data:image/png;base64,>-->
        </a>
        <div class="content" >
          <a class="author" style="font-size: 2em;"><%=review.customer.name.humanize %>
            <div  style="margin-left: 350px">
              <%if current_user.accountable == review.customer %>
                <%=link_to 'Delete', review_path(id: review.id , product_id: @product.id), data:{turbo_confirm:"Are you sure , do you want to delete this comment!" , turbo_method: :delete}, class:"btn btn-danger" %>
              <%end %>
            </div>

          </a>
          <%if review.customer == current_user.accountable %>
            <div class="text" style="font-size: 1.5em ; margin-top: -20px; margin-left: 30px">
              <p>Rating : <%=review.rating %> / 5</p>
              <p>Comment : <%=review.comment %></p>
            </div>
          <%else %>
            <div class="text" style="font-size: 1.5em ; margin-top: 10px; margin-left: 30px">
              <p>Rating : <%=review.rating %> / 5</p>
              <p>Comment : <%=review.comment %></p>
            </div>
          <%end %>
        </div>
      </div>
    <%end %>
  </div>
  <div class="ui large four column celled table" style=" margin-left: 43% ; width: 250px; height: 40px ; font-size: 1.5em; color: orange ;">
    <center style="padding-left: 15%">
      <%= paginate @reviews %>
    </center>
  </div>
<%end %>


<br><br><br>